{
  "name": "kleidi.ca",
  "type": "module",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview --host",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/rss": "4.0.1",
    "@astrojs/sitemap": "3.0.3",
    "@astrojs/svelte": "4.0.4",
    "@astrojs/tailwind": "5.1.0",
    "astro": "3.6.4",
    "astro-compress": "2.2.3",
    "astro-icon": "0.8.2",
    "astro-robots-txt": "1.0.0",
    "remark-toc": "9.0.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@astrojs/check": "0.4.1",
    "prettier": "3.1.1",
    "prettier-plugin-astro": "0.12.2",
    "prettier-plugin-svelte": "3.1.2",
    "svgo": "2.8.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      },
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "proseWrap": "always"
  }
}
