{
  "name": "kleidi.ca",
  "type": "module",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview --host",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/rss": "4.0.5",
    "@astrojs/sitemap": "3.1.1",
    "@astrojs/svelte": "5.2.0",
    "astro": "4.6.3",
    "astro-compress": "2.2.11",
    "astro-robots-txt": "1.0.0",
    "remark-toc": "9.0.0"
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-svelte": "3.2.2"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      },
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "proseWrap": "always"
  }
}
